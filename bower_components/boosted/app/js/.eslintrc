{
  "env": {
    "browser": true,
    "builtin": true,
    "jquery": true
  },
  "globals": {},
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 0,
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      0,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      0,
      "smart"
    ],

    "guard-for-in": 2,
    "new-cap": 0,
    "no-bitwise": 0,
    "no-caller": 1,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      0,
      "never"
    ],
    "strict": [
      2,
      "function"
    ],
    "valid-typeof": 2,
    "no-unreachable": 0
  }
}
